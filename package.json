{
  "name": "extract-pg-schema",
  "version": "3.2.0",
  "keywords": [
    "postgresql",
    "schema"
  ],
  "repository": "git@github.com:kristiandupont/extract-pg-schema.git",
  "license": "MIT",
  "author": {
    "name": "Kristian Dupont",
    "url": "http://www.kristiandupont.com"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "yarn eslint . -f visualstudio --report-unused-disable-directives --ext .js,.ts",
    "test": "vitest run",
    "version": "yarn build"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/*.test.js"
    ],
    "transform": {
      "^.+\\.(ts|js)$": "ts-jest"
    }
  },
  "dependencies": {
    "jsonpath": "^1.0.2",
    "knex": "2.1.0",
    "pg": "8.7.3",
    "pg-query-emscripten": "^0.1.0",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@kristiandupont/eslint-config": "1.2.7",
    "@types/pg": "8.6.5",
    "@types/ramda": "0.28.13",
    "eslint": "8.16.0",
    "np": "7.6.1",
    "prettier": "2.6.2",
    "testcontainers": "8.10.1",
    "typescript": "4.7.2",
    "vitest": "0.14.2"
  },
  "np": {}
}
