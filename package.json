{
  "name": "extract-pg-schema",
  "version": "1.9.0",
  "keywords": [
    "postgresql",
    "schema"
  ],
  "repository": "git@github.com:kristiandupont/extract-pg-schema.git",
  "license": "MIT",
  "author": {
    "name": "Kristian Dupont",
    "url": "http://www.kristiandupont.com"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "yarn eslint . -f visualstudio --report-unused-disable-directives --ext .js,.ts",
    "test": "jest",
    "version": "yarn build"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/*.test.js"
    ],
    "transform": {
      "^.+\\.(ts|js)$": "ts-jest"
    }
  },
  "dependencies": {
    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/knex": "0.16.1",
    "@types/ramda": "0.27.36",
    "@typescript-eslint/parser": "4.14.2",
    "eslint": "7.20.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "knex": "0.21.17",
    "np": "7.2.0",
    "pg": "8.5.1",
    "prettier": "2.2.1",
    "testcontainers": "7.3.0",
    "ts-jest": "26.5.0",
    "typescript": "4.1.3"
  },
  "np": {}
}
