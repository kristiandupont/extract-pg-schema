{
  "name": "extract-pg-schema",
  "version": "3.0.1",
  "keywords": [
    "postgresql",
    "schema"
  ],
  "repository": "git@github.com:kristiandupont/extract-pg-schema.git",
  "license": "MIT",
  "author": {
    "name": "Kristian Dupont",
    "url": "http://www.kristiandupont.com"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "yarn eslint . -f visualstudio --report-unused-disable-directives --ext .js,.ts",
    "test": "jest",
    "version": "yarn build"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/*.test.js"
    ],
    "transform": {
      "^.+\\.(ts|js)$": "ts-jest"
    }
  },
  "dependencies": {
    "@types/pg": "^8.0.0",
    "knex": "0.95.6",
    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/ramda": "0.27.40",
    "@typescript-eslint/parser": "4.24.0",
    "eslint": "7.26.0",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "26.6.3",
    "np": "7.5.0",
    "pg": "8.6.0",
    "prettier": "2.3.0",
    "testcontainers": "7.11.0",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "np": {}
}
